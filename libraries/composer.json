{
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "vendor-dir": "internal-vendor",
    "preferred-install": {
      "*": "dist"
    },
    "platform-check": false,
    "autoloader-suffix": "Revisionary"
  },
  "require": {
    "php": ">=7.2.5",
    "publishpress/vendor-locator-revisions": "^1.0",
    "publishpress/pimple-pimple": "^3.5.0.9",
    "publishpress/psr-container": "^2.0.1.9",
    "publishpress/instance-protection": "^2.0",
    "publishpress/wordpress-reviews":  "^1.1",
    "publishpress/wordpress-version-notices": "^2.1"
  }
}